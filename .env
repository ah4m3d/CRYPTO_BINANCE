# Add your real Binance API credentials here (not required for public market data)
BINANCE_API_KEY=dummy_api_key_for_testing
BINANCE_SECRET_KEY=dummy_api_secret_for_testing

# Use testnet for development (set to false for live trading)
BINANCE_TESTNET=true

# WebSocket URLs
BINANCE_WS_URL=wss://stream.binance.com:9443/ws
BINANCE_TESTNET_WS_URL=wss://testnet.binance.vision/ws

# API URLs  
BINANCE_API_URL=https://api.binance.com
BINANCE_TESTNET_API_URL=https://testnet.binance.vision

# Server Configuration
SERVER_PORT=8080
PORT=8080

# Database Configuration (PostgreSQL)
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=password
DB_NAME=trading_engine
DB_SSL_MODE=disable

# Redis Configuration (optional)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0
